{
  "name": "{{projectName}}",
  "version": "0.0.1",
  "description": "Wingbot chatbot: {{projectName}}",
  "main": "handler.js",
  "scripts": {
    "start": {{#if isAwsOrAzure}}"echo \"Error: start command not available\" && exit 1",
{{/if}}{{#if expressOrAppService}}"node ./bin/www.js",{{/if}}
    "test": "npm run test:lint && npm run test:coverage",
    "test:coverage": "nyc --reporter=html mocha ./test && nyc report",
    "test:coverage:threshold": "nyc check-coverage --lines 80 --functions 80 --branches 75",
    "test:bot": "mocha ./test",
    "test:lint": "eslint --ext .js src test bot routes bin *.js",
    "build:test": ":",
    "build:production": ":"{{#if awsServerless}},
    "deploy:test": "npm run build:test && sls deploy -s test",
    "deploy:production": "npm run build:production && sls deploy -s production"{{/if}}
  },
  "keywords": [
    "wingbot",
    "chatbot"
  ],
  "author": "Wingbot.ai",
  "devDependencies": { {{#if awsServerless}}
    "aws-sdk": "^2.101.0",{{else}}{{#if mongodbStorage}}"aws-sdk": "^2.101.0",{{/if}}{{/if}}
    "env-test": "^1.0.0",
    "eslint": "^4.16.0",{{#if eslint}}
    "eslint-config-airbnb": "^16.1.0",
    "eslint-mocha": "^0.4.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.6.1",{{/if}}
    "mocha": "^5.1.1",
    "nyc": "^11.7.3"
  },
  "dependencies": { {{#if expressOrAppService}}
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "http-errors": "^1.6.3",
    "morgan": "^1.9.0",{{/if}}{{#if isMongoOrCosmos}}
    "mongodb": "^3.0.8",
    "mongodb-core": "^3.0.8",{{/if}}
    "wingbot": "^1.0.0"{{#if dynamodbStorage}},
    "wingbot-dynamodb": "*"{{/if}}{{#if jwtTokenStorage}},
    "wingbot-jwt": "*"{{/if}}{{#if messenger}},
    "wingbot-facebook": "^1.0.0"{{/if}}{{#if botService}},
    "wingbot-botservice": "^1.0.0-alpha.10"{{/if}}{{#if isMongoOrCosmos}},
    "wingbot-mongodb": "*"{{/if}}{{#if googleAnalytics}},
    "universal-analytics": "^0.4.16"{{/if}}{{#if analytics}},
    "webalize": "^0.1.0"{{/if}}
  }
}