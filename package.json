{
  "name": "wingbot-cli",
  "version": "2.5.1",
  "description": "Wingbot.ai CLI tool",
  "main": "main.js",
  "bin": {
    "wingbot": "./bin/wingbot.js"
  },
  "scripts": {
    "doc": "node ./bin/makeApiDoc.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:coverage": "nyc --reporter=html mocha --opts ./test/mocha.opts ./test && nyc report",
    "test:coverage:threshold": "nyc check-coverage --lines 80 --functions 80 --branches 74",
    "test:unit": "mocha --opts ./test/mocha.opts ./test",
    "test:lint": "eslint --ext .js bin src test *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wingbotai/wingbot-cli.git"
  },
  "keywords": [
    "FastText",
    "ML",
    "AI",
    "Machine",
    "Learning"
  ],
  "author": "David Menger",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.3.4",
    "chalk": "^2.4.1",
    "cli-spinner": "^0.2.8",
    "commander": "^2.19.0",
    "event-stream": "^4.0.1",
    "glob": "^7.1.3",
    "handlebars": "^4.0.12",
    "inquirer": "^6.2.0",
    "latest-version": "^4.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "xml-nodes": "^0.1.5",
    "xml-objects": "^1.0.1",
    "xml-stream": "^0.4.5"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-mocha": "^0.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsdoc": "^3.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "^7.11.1",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "sinon": "^6.3.5"
  }
}
