{
  "name": "wingbot-cli",
  "version": "2.9.0",
  "description": "Wingbot.ai CLI tool",
  "main": "main.js",
  "bin": {
    "wingbot": "./bin/wingbot.js"
  },
  "scripts": {
    "doc": "node ./bin/makeApiDoc.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:coverage": "nyc --reporter=html mocha --opts ./test/mocha.opts ./test && nyc report",
    "test:coverage:threshold": "nyc check-coverage --lines 80 --functions 80 --branches 74",
    "test:unit": "mocha --opts ./test/mocha.opts ./test",
    "test:lint": "eslint --ext .js bin src test *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wingbotai/wingbot-cli.git"
  },
  "keywords": [
    "FastText",
    "ML",
    "AI",
    "Machine",
    "Learning"
  ],
  "author": "David Menger",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.3.5",
    "chalk": "^2.4.2",
    "cli-spinner": "^0.2.10",
    "commander": "^2.20.0",
    "density-clustering": "^1.3.0",
    "event-stream": "^4.0.1",
    "glob": "^7.1.4",
    "handlebars": "^4.1.2",
    "inquirer": "^6.3.1",
    "latest-version": "^5.1.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "webalize": "^0.1.0",
    "xml-nodes": "^0.1.5",
    "xml-objects": "^1.0.1",
    "xml-stream": "^0.4.5"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-mocha": "^0.5.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsdoc": "^5.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-react": "^7.13.0",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2"
  },
  "optionalDependencies": {
    "fast-text": "^1.0.0-alpha.2"
  }
}
